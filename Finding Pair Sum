#include <iostream>
using namespace std;
int main(){
    int S = 0;
    int A[5] = {2, -3, 3, 3, -2};
    for (int i =0; i<5; i++){
        for (int j = i+1; j<5; j++){
            if (A[i] + A[j] == S){
                cout<< A[i] << "," << A[j] << "\n";
            }
            }
            
        }
    }
