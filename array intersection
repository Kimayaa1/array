#include <iostream>
#include <climits>
using namespace std;
int main(){
    int A[5] = {2,3,4,6,9};
    int B[3] = {1,4,7};
    for (int i =0; i<5; i++){
        for (int j = 0; j<3; j++){
            if(A[i] == B[j]){
                cout<< A[i];
                A[i] = INT_MIN;
                break;
            }
            
        }
    }
}
